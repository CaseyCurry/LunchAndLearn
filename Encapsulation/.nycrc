{
  "include": ["src/**/*.js"],
  "exclude": [
    "src/**/*-spec.js"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".js"
  ],
  "require": [
    "babel-core/register"
  ],
  "cache": true,
  "check-coverage": false,
  "report-dir": "./spec/coverage",
  "sourcemap": true,
  "instrument": true
}
